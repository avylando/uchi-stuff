{
  "name": "graph-syntax-vscode",
  "displayName": "graph-syntax-vscode",
  "description": "Colourize graph files syntax",
  "version": "0.0.1",
  "publisher": "Alexij",
  "engines": {
    "vscode": "^1.23.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "graph",
        "aliases": ["Graph", "graph"],
        "extensions": [".graph"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "graph",
        "scopeName": "source.graph",
        "path": "./syntaxes/graph.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "graph",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
